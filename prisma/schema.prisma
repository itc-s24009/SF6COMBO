generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Combo {
  id        String   @id @default(cuid())
  moves     String   
  endFrame  Int?     
  damage    Int?     
  remarks   String?  
  tags      Tag[]    
  createdAt DateTime @default(now())
}

model Tag {
  id        String   @id @default(cuid())
  name      String   @unique
  combos    Combo[]
}